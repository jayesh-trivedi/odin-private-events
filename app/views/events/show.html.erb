<h1><%= @event.title %></h1><br>
<h2><%= "By #{User.find(@event.creator_id).username}"%></h2><br>

<div><%= @event.description %></div>
<div><%= "Date - #{@event.date.strftime("%m/%d/%Y")}"%></div>

<%= link_to 'Attend Event!', attend_event_path %>

<h3>Event Attendees</h3>
<% @event.attendees.each do |attendee| %>
  <p><%= attendee.username %><p>
<% end %>
